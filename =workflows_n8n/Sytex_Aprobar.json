    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT \n    SF.codigo_workflow AS wbs,\n    SF.codigo_formulario AS form,\n    SF.estado_formulario AS estado,\n\n    -- Pivot de respuestas\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Describa tarea a realizar:' THEN SR.valor_respuesta END) AS tarea,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Indique Ejecutante' THEN SR.valor_respuesta END) AS ejecutante,\n    MAX(CASE WHEN SR.etiqueta_pregunta = '¿Afecta a Clientes Mercado Empresa?' THEN SR.valor_respuesta END) AS afecta_clientes,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Hora de inicio:' THEN SR.valor_respuesta END) AS hora_inicio,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Área requirente de Claro:' THEN SR.valor_respuesta END) AS requirente,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Razón Social Proveedor:' THEN SR.valor_respuesta END) AS proveedor,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Fecha estimada de ejecución:' THEN SR.valor_respuesta END) AS fecha_estimada,\n    MAX(CASE WHEN SR.etiqueta_pregunta = '¿La tarea requiere ACCIÓN de SAE?' THEN SR.valor_respuesta END) AS accion_sae\n\nFROM sytex_formularios AS SF\nJOIN sytex_respuestas AS SR \n    ON SR.codigo_formulario = SF.codigo_formulario\n    AND SR.etiqueta_pregunta IN (\n        'Describa tarea a realizar:',\n        'Indique Ejecutante',\n        '¿Afecta a Clientes Mercado Empresa?',\n        'Hora de inicio:',\n        'Área requirente de Claro:',\n        'Razón Social Proveedor:',\n        'Fecha estimada de ejecución:',\n        '¿La tarea requiere ACCIÓN de SAE?'\n    )\nWHERE SF.estado_formulario = 'Enviado'\nGROUP BY \n    SF.codigo_workflow,\n    SF.codigo_formulario,\n    SF.estado_formulario;\n",
        "options": {}
      },
