    {
      "parameters": {
        "jsCode": "// Recorre cada elemento de entrada\nfor (const item of $input.all()) {\n  const t = item.json;\n\n  // Normaliza valores por seguridad\n  const accionSAE = t.accion_sae?.trim().toLowerCase();\n  const ejecutante = t.ejecutante?.trim().toLowerCase();\n\n  // Lógica de negocio para el nuevo campo \"aprobar\"\n  if (accionSAE !== \"si\" && ejecutante !== \"soporte acceso empresas - sae\") {\n    t.aprobar = \"Ingeniero\";\n  } else {\n    t.aprobar = \"Especialista\";\n  }\n\n  // Se guarda el cambio en el mismo objeto\n  item.json = t;\n}\n\n// Devuelve todos los items modificados\nreturn $input.all();\n"
