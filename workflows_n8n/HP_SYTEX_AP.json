    {
      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT \n    SF.codigo_workflow AS wbs,\n    SF.codigo_formulario AS form,\n    SF.estado_formulario AS estado,\n\n    -- Pivot de respuestas\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Describa tarea a realizar:' THEN SR.valor_respuesta END) AS tarea,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Indique Ejecutante' THEN SR.valor_respuesta END) AS ejecutante,\n    MAX(CASE WHEN SR.etiqueta_pregunta = '¿Afecta a Clientes Mercado Empresa?' THEN SR.valor_respuesta END) AS afecta_clientes,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Hora de inicio:' THEN SR.valor_respuesta END) AS hora_inicio,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Área requirente de Claro:' THEN SR.valor_respuesta END) AS requirente,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Razón Social Proveedor:' THEN SR.valor_respuesta END) AS proveedor,\n    MAX(CASE WHEN SR.etiqueta_pregunta = 'Fecha estimada de ejecución:' THEN SR.valor_respuesta END) AS fecha_estimada,\n    MAX(CASE WHEN SR.etiqueta_pregunta = '¿La tarea requiere ACCIÓN de SAE?' THEN SR.valor_respuesta END) AS accion_sae,\n\n    -- Nuevo campo: código del proyecto (solo uno por formulario)\n    MIN(SR.codigo_proyecto) AS codigo_proyecto\n\nFROM sytex_formularios AS SF\nJOIN sytex_respuestas AS SR \n    ON SR.codigo_formulario = SF.codigo_formulario\n    AND SR.etiqueta_pregunta IN (\n        'Describa tarea a realizar:',\n        'Indique Ejecutante',\n        '¿Afecta a Clientes Mercado Empresa?',\n        'Hora de inicio:',\n        'Área requirente de Claro:',\n        'Razón Social Proveedor:',\n        'Fecha estimada de ejecución:',\n        '¿La tarea requiere ACCIÓN de SAE?'\n    )\nWHERE SF.estado_formulario = 'Enviado'\nGROUP BY \n    SF.codigo_workflow,\n    SF.codigo_formulario,\n    SF.estado_formulario;\n",
        "options": {}
      },
      "type": "n8n-nodes-base.mySql",
      "typeVersion": 2.5,
      "position": [
        -192,
        -176
      ],
      "id": "7c6e5406-fd35-46f3-8c44-98acd25a5232",
      "name": "Execute a SQL query",
      "credentials": {
        "mySql": {
          "id": "uNW6TD2qlwo9AR01",
          "name": "MySQL crm"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Recorre cada elemento de entrada\nconst tareas = [];\n\nfor (const item of $input.all()) {\n  const t = item.json;\n\n  // Normaliza valores por seguridad\n  const accionSAE = t.accion_sae?.trim().toLowerCase();\n  const ejecutante = t.ejecutante?.trim().toLowerCase();\n\n  // Lógica de negocio para el nuevo campo \"aprobar\"\n  if (accionSAE !== \"si\" && ejecutante !== \"soporte acceso empresas - sae\") {\n    t.aprobar = \"Ingeniero\";\n  } else {\n    t.aprobar = \"Especialista\";\n  }\n\n  // Nueva lógica: asignar nombre de proyecto según el código\n  switch (t.codigo_proyecto) {\n    case \"PR-23-0279\":\n      t.proyecto = \"SAE-CRÍTICO-IMPLEMENTACIONES ISA\";\n      break;\n    case \"PR-23-0996\":\n      t.proyecto = \"SAE-NO CRÍTICO-IMPLEMENTACIONES ISA\";\n      break;\n    case \"PR-23-0343\":\n      t.proyecto = \"SAE-CRÍTICO-CONFIGURACION/UPGRADES\";\n      break;\n    case \"PR-23-0344\":\n      t.proyecto = \"SAE-NO CRÍTICO-CONFIGURACION/UPGRADES\";\n      break;\n    case \"PR-23-0345\":\n      t.proyecto = \"SAE-EMERGENCIA DE RED\";\n      break;\n    default:\n      t.proyecto = \"SIN CLASIFICAR\";\n  }\n\n  // Agrega el resultado al arreglo de tareas\n  tareas.push({\n    wbs: t.wbs,\n    form: t.form,\n    estado: t.estado,\n    tarea: t.tarea,\n    ejecutante: t.ejecutante,\n    afecta_clientes: t.afecta_clientes,\n    hora_inicio: t.hora_inicio,\n    requirente: t.requirente,\n    proveedor: t.proveedor,\n    fecha_estimada: t.fecha_estimada,\n    accion_sae: t.accion_sae,\n    codigo_proyecto: t.codigo_proyecto,\n    proyecto: t.proyecto,\n    aprobar: t.aprobar\n  });\n}\n\n// Devuelve la salida en el formato solicitado\nreturn [\n  {\n    json: {\n      tareas: tareas\n    }\n  }\n];\n"
      },
