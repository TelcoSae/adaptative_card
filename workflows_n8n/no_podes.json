    {
      "parameters": {
        "functionCode": "// Filtrar notas relevantes y ordenar\nlet notes = $json.filter(n => n['Work Log Type'] === 'General Information');\nnotes.sort((a,b) => new Date(a['Submit Date']) - new Date(b['Submit Date']));\n\n// Última nota\nconst lastNote = notes[notes.length - 1] || {};\n\n// Concatenar todo para IA\nconst textForAI = notes.map(n => `${n['Submit Date']}: ${n['Detailed Description']}`).join('\\n\\n');\n\nreturn [{json:{textForAI, lastNote}}];"
      },
