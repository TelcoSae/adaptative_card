    {
      "parameters": {
        "functionCode": "const keywords = ['status','actualización','avance','novedad','novedades'];\nconst message = $json['body']?.toLowerCase() || '';\nconst regex = /INC\\d{12}/;\nconst match = message.match(regex);\nif(match && keywords.some(k => message.includes(k))){\n  return [{json:{incNumber: match[0]}}];\n}\nreturn [];"
      },
