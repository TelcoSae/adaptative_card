      "parameters": {
        "operation": "executeQuery",
        "query": "SELECT \n  alarmas.horario, \n  UPPER(nodos.SysName) AS sysName, \n  alarmas.sourceName,\n  CASE\n  WHEN alarmas.regional = 'nnmreg03' THEN \n  (SELECT descripcion FROM NNMi.interfaces_agg WHERE UUID = alarmas.node_uuid AND interface = alarmas.sourceName)\n  ELSE nodos.sysLocation\n  END AS sysLocation,\n  IFNULL(AnilloCritico.iconos, '') AS Critico\n  fROM \n  NNMi.nodos\n  JOIN \n  NNMi.alarmas\n  ON nodos.uuid = alarmas.node_uuid\n  LEFT JOIN\n  NNMi.AnilloCritico\n  ON nodos.sysLocation = AnilloCritico.sysLocation\n  WHERE \n  alarmas.state = 'Registered'\n  AND alarmas.alarmtype = 'InterfaceDown'\n  AND (CASE\n  WHEN alarmas.regional = 'nnmreg03' THEN \n  (SELECT descripcion FROM NNMi.interfaces_agg WHERE UUID = alarmas.node_uuid AND interface = alarmas.sourceName)\n  ELSE nodos.sysLocation\n  END) LIKE 'ME-%'\n  ORDER BY \n  horario\n",
        "options": {}
      },
